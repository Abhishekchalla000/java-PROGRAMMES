class A extends Thread
{
	public void run()
	{
		int i;
	for(i=1;i<=10;i++) 
		{
		System.out.println("hiii");
		}	
	}
}
class B extends Thread
{
	public void run()
	{
		int i;
	for(i=1;i<=10;i++) 
		{
		System.out.println("hiii");
		}	
	}
}

public class ABC {

	public static void main(String[] args) {
		A obj1= new A();
		B obj2=new B();
		
		obj1.start();
		obj2.start();
	}

}
